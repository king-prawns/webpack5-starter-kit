{
  "name": "webpack5-starter-kit",
  "version": "2.0.0",
  "description": "Webpack 5 starter kit",
  "main": "./src/index.js",
  "scripts": {
    "dev": "node ./config/dev",
    "build": "node ./config/prod",
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "jest --coverage --silent",
    "prestart": "yarn build",
    "start": "http-server ./dist -p 8082 --silent"
