    "postcss-reporter": "7.0.5",
    "postcss-responsive-type": "1.0.0",
    "style-loader": "3.3.2",
    "stylelint": "15.4.0",
    "stylelint-config-standard": "32.0.0",
    "stylelint-webpack-plugin": "4.1.0",
    "terser-webpack-plugin": "5.3.7",
    "url-loader": "4.1.1",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2",
