{
  "name": "webpack5-starter-kit",
  "version": "2.0.0",
  "description": "Webpack 5 starter kit",
  "main": "./src/index.js",
  "scripts": {
    "dev": "node ./config/dev",
    "build": "node ./config/prod",
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "nyc ava",
    "prestart": "yarn build",
    "start": "http-server ./dist -p 8082 --silent"
  },
  "author": "Giorgio Gamberoni <giorgio.gamberoni@gmail.com> (https://github.com/king-prawns)",
  "license": "MIT",
  "homepage": "https://github.com/king-prawns/webpack5-starter-kit#readme",
  "ava": {
    "files": [
      "./src/**/*test.js"
    ],
    "failFast": true,
    "failWithoutAssertions": false,
    "require": [
      "@babel/register"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-flow": "7.18.6",
    "@babel/register": "7.18.9",
    "@babel/runtime": "7.18.9",
    "autoprefixer": "10.4.8",
    "ava": "4.3.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "chalk": "4.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "cssnano": "5.1.12",
    "cssnano-preset-advanced": "5.3.8",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "file-loader": "6.2.0",
    "flow-bin": "0.184.0",
    "fs-extra": "10.1.0",
    "html-loader": "4.1.0",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.1.1",
    "mini-css-extract-plugin": "2.6.1",
    "nyc": "15.1.0",
    "opn": "6.0.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss": "8.4.16",
    "postcss-easy-import": "4.0.0",
    "postcss-load-config": "4.0.1",
    "postcss-loader": "7.0.1",
    "postcss-preset-env": "7.7.2",
    "postcss-reporter": "7.0.5",
    "postcss-responsive-type": "1.0.0",
    "style-loader": "3.3.1",
    "stylelint": "14.10.0",
    "stylelint-config-standard": "27.0.0",
    "stylelint-webpack-plugin": "3.3.0",
    "terser-webpack-plugin": "5.3.4",
    "url-loader": "4.1.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.10.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@babel/eslint-parser": "7.18.9"
  }
}
