    "postcss-reporter": "7.1.0",
    "postcss-responsive-type": "1.0.0",
    "style-loader": "4.0.0",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-webpack-plugin": "5.0.1",
    "terser-webpack-plugin": "5.3.14",
    "url-loader": "4.1.1",
    "webpack": "5.101.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2",
    "webpack-merge": "6.0.1"
